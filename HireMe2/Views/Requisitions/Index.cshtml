@model IEnumerable<HireMe2.Models.Requisition>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Requisitions</h2>
<table>
    <thead>
        <tr>
            <th>Requisition</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var requisition in Model)
        {
            <tr>
                <td>@Html.ActionLink(requisition.Title, "Details", "Requisitions", new { id = requisition.Id}, null)</td>
                <td>@requisition.Genre.Name</td>
            </tr>
        }
    </tbody>
</table>
